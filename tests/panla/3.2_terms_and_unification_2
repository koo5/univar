#â€” Existence of lists
(?X).
{ [ ] rdf:first ?X; rdf:rest ?L} <= { ?L rdf:rest [ ] }.

:banana a :Nat.

#:zero a :Nat.
#{?x a :Nat} => {?x :has_succ ?y. ?y a :Nat}.
{?l1_0 :has_sum ?y.} <= {(:zero ?y). ?y a :Nat}.
#{(?sx ?y) :has_sum ?sz } <= {?x :has_succ ?sx. ?z :has_succ ?sz. (?x ?y) :has_sum ?z.}.
fin.

query
(:zero :banana) :has_sum :banana.
fin.
shouldbe
(:zero :banana) :has_sum :banana.
fin.

query
(:three :four) :has_sum :seven.
fin.
shouldbe
(:three :four) :has_sum :seven.
fin.

query
(:four :three) :has_sum :seven.
fin.
shouldbe
(:four :three) :has_sum :seven.
fin.

query
(:three :four) :has_sum ?w.
fin.
shouldbe
(:three :four) :has_sum :seven.
fin.

query
(:three ?w) :has_sum :seven.
fin.
shouldbe
(:three :four) :has_sum :seven.
fin.

