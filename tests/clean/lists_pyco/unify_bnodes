#â€” Existence of lists
?builtins_aware_list rdf:first ?ITEM; rdf:rest ?REST.

:T :not :F.
:F :not :T.

{?y :not ?ny. ?z :not ?nz. ?l0 rdf:first ?y. ?l0 rdf:rest ?l1. ?l1 rdf:first ?z. ?l1 rdf:rest rdf:nil} => { ?l0 :a :SAT2 }.

?EX :eq ?EX.

{?x :a :SAT} <= {?x :a :SAT2. ?y :a :SAT2. ?x :eq ?y.}.

fin.
query
(?x ?y) :a :SAT.
fin.
shouldbe
(:T :T) :a :SAT.
fin.
shouldbe
(:T :F) :a :SAT.
fin.
shouldbe
(:F :T) :a :SAT.
fin.
shouldbe
(:F :F) :a :SAT.
fin.
thatsall

