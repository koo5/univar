:this :a :thing. :that :a :thing2.
{ ?z :a :thing. ?l0 rdf:first ?y. ?l0 rdf:rest ?l1. ?l1 rdf:first ?z. ?l1 rdf:rest rdf:nil. ?y :a :thing2} => { ?l0 :a :potato }.


