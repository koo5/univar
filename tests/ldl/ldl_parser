
#ldl:parse_nonterminal (picks a rule)
#->ldl:try_rule grammar
#  ->:parse_rhs
#    ->:parse_grammar_items
#      ->:parse_grammar_item builtins/string
#      ->:parse_grammar_item builtins/identifier --> ldl:parse_nonterminal
#      ->:parse_grammar_item builtins/any_char_except
#      ->:parse_grammar_item builtins/all_chars_until_one_of
#      ->:parse_grammar_item builtins/a_series_of_y_separated_with_z
#        ->:is_series ---> ldl:parse_nonterminal
#      ->:parse_grammar_item builtins/postprocessed ..--> ldl:parse_nonterminal

{(?x0 ?grammar ?nont) ldl:parse_nonterminal (?rest ?ast ?depth)} <= {
	?depth_predecessor :has_succ ?depth.
	?rule :memberof ?grammar.
	(?x0 ?grammar ?rule ?nont) ldl:try_rule (?rest ?ast ?depth).
}.

{(?x0 ?grammar ?rule ?nont) ldl:try_rule (?rest ?ast ?depth)} <= {
    ?rule :type "builtins/grammar"; ldl:children ([:value [ldl:contents ?nont]] [:value [ldl:contents ?rhs]]).
	(?x0 ?grammar ?nont ?rhs) :parse_rhs (?rest ?ast ?depth).
}.

{(?x0 ?grammar ?type rdf:nil) :parse_rhs (?x0 ?ast ?depth)} <= {
	?ast a ldl:compound; :type ?type; ldl:children rdf:nil.
}.

{(?x0 ?grammar ?type ?rhs) :parse_rhs (?rest ?ast ?depth)} <= {
    ?rhs rdf:rest ?rhs2_not_nil. ?rhs2_not_nil rdf:rest ?rhs3.
	?ast a ldl:compound; :type ?type; ldl:children ?ch.
	(?x0 ?grammar ?rhs) :parse_grammar_items (?rest ?ch ?depth).
}.

{(?x0 ?grammar ?rhs_) :parse_grammar_items (?rest ?children_ ?depth)} <= {
	(?x0 ?grammar ?rhsf) :parse_grammar_item (?after_item ?childrenf ?depth).
	(?after_item ?grammar ?rhsr) :parse_grammar_items (?rest ?childrenr ?depth).
}.

{(?x0 ?grammar rdf:nil) :parse_grammar_items (?x0 rdf:nil ?depth)} <= {}.

{(?x0 ?grammar ?rhs_item) :parse_grammar_item (?rest ?child ?depth)} <= {
	?rhs_item a ldl:value; ldl:type "builtins/string"; ldl:contents ?v.
	?child eq ?rhs_item.
	:dummy :literally_string (?x0 ?rest ?v).
}.

{(?x0 ?grammar ?rhs_item) :parse_grammar_item (?rest ?child ?depth)} <= {
	?rhs_item a ldl:value; ldl:type "builtins/identifier"; ldl:contents ?value.
	?child :eq [:key ?value; :value ?ast].
    (?x0 ?grammar ?value) ldl:parse_nonterminal (?rest ?ast ?depth).
}.

{(?x0 ?grammar ?rhs_item) :parse_grammar_item (?rest ?child ?depth)} <= {
	?rhs_item :type "builtins/node_name_with_child_name".
	?rhs_item ldl:children ?gch.
	[:key "child_name"; :value [ldl:contents ?child_name]] :memberof ?gch.
    [:key "type_name"; :value [ldl:contents ?type_name]] :memberof ?gch.
    ?child :eq [:key ?child_name; :value ?ast].
    (?x0 ?grammar ?value) ldl:parse_nonterminal (?rest ?ast ?depth).
}.

{(?x_ ?grammar ?rhs_item) :parse_grammar_item (?xr ?ast ?depth)} <= {
	?rhs_item :type "builtins/any_char_except".
	[:key "except"; :value [ldl:contents ?except]] :memberof ?rhs_item!ldl:children.
	?xf string_builtins:any_char_except ?except.
 	?ast a ldl:value; ldl:type "builtins/string"; ldl:contents ?xf.
}.

{(?x0 ?grammar ?rhs_item) :parse_grammar_item (?rest ?ast ?depth)} <= {
	?rhs_item :type "builtins/all_chars_until_one_of".
	[:key "until"; :value [ldl:contents ?except]] :memberof ?rhs_item!ldl:children.
 	?ast a ldl:value; ldl:type "builtins/string"; ldl:contents ?string.
	?x0 :all_chars_until_one_of (?rest ?except ?char_list).
    ?string string_builtins:strXlst ?char_list.
}.

{(?x0 ?grammar ?rhs_item) :parse_grammar_item (?rest ?child ?depth)} <= {
	?rhs_item :type "builtins/a_series_of_y_separated_with_z"; ldl:children ?grammar_children.
	[:key "item_type"; :value ?item_type] :memberof ?grammar_children.
	[:key "separator"; :value ?separator] :memberof ?grammar_children.
	:dummy :is_series (?x0 ?rest ?grammar ?item_type ?separator ?child ?depth).
}.

{(?x0 ?grammar ?rhs_item) :parse_rhs_item (?rest ?child ?depth)} <= {
	?rhs_item :type "builtins/builtin_postprocessed".
	?rhs_item ldl:children ([:value [ldl:contents ?raw_identifier]]).
	(?x0 ?grammar ?raw_identifier) :parse_nonterminal (?rest ?raw_ast ?depth).
	?raw_ast :postprocess ?ast.
}.

{?raw :postprocess ?cooked} <= {
    ?raw ldl:type "builtins/string_literal_raw"; ldl:children ?ch.
    ?cooked :eq [a ldl:value; ldl:type "builtins/string"; ldl:contents ?v].
    ?v_list :is_collected_string_literal_raw_body ?raw.
    ?v string_builtins:strXlst ?ch.
}.

{rdf:nil :is_collected_string_literal_raw_body rdf:nil} <= {}.

{?v_ :is_collected_string_literal_raw_body ?r_} <= {
    ?rf :eq [:contents ?vf].
    ?vr :is_collected_string_literal_raw_body ?rr.
}.





{:dummy :shorten_location_printout (?x0 ?shortened)} <= {
    :dummy :from_x_put_first_y_items_into_z (?x0 :n42 ?shortened).
}.



{:dummy :is_series (?x0 ?rest ?grammar ?item_type ?separator ?ast_ ?depth)} <= {
    (?x0  ?grammar ?item_type) ldl:parse_nonterminal (?mid ?astf ?depth).
    (?mid ?grammar ?separator) ldl:parse_nonterminal (?end ?separator_ast ?depth).
    :dummy :is_series (?end ?rest ?grammar ?item_type ?separator ?astr ?depth).
}.

{:dummy :is_series (?x0 ?rest ?grammar ?item_type ?separator ?ast_ ?depth)} <= {
    (?x0  ?grammar ?item_type) ldl:parse_nonterminal (?rest ?astf ?depth).
    astr :eq rdf:nil.
}.

{:dummy :is_series (?x0 ?x0 ?grammar ?item_type ?separator rdf:nil ?depth)} <= {
}.



{?x_ :all_chars_until_one_of (?rest ?except ?char_list_).} <= {
    ?xf string_builtins:any_char_except ?except.
    ?char_listf :eq ?xf.
    ?xr :all_chars_until_one_of (?rest ?except ?char_listr).
}.

{rdf:nil :all_chars_until_one_of (rdf:nil ?except rdf:nil).} <= {
}.

{?x_ :all_chars_until_one_of (?x_ ?except rdf:nil).} <= {
    ?except string_builtins:strXlst ?except_list.
    ?xf :memberof ?except_list.
}.



		#:dummy tau_builtins:output "testing rule lhs...".
		#:dummy tau_builtins:serialize_thing ("found rule: " ?rule!:description)^string_builtins:strXlst.
	    #:dummy tau_builtins:output ?depth.
		#:dummy :shorten_location_printout (?x0 ?x0_shortened).
		#?x0_shortened_string string_builtins:strXlst ?x0_shortened.
		#:dummy tau_builtins:serialize_thing ("""invoking rule on """" ?x0_shortened_string """"...""")^string_builtins:strXlst.



